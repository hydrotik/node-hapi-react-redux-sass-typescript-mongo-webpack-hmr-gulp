    "node": ">=4.2.0",
    "npm": ">=3.3.0"
  },
  "engineStrict": true,
  "scripts": {
    "postinstall": "node node_modules/typings/dist/bin.js install",
    "build-client": "npm run clean; npm run lint-es; cross-env NODE_ENV=production ./node_modules/.bin/webpack -p --verbose --colors --display-error-details --config webpack.config.client-hot.js",
    "clean": "node ./script/clean.js",
    "lint-ts": "tslint --config tslint.json --ext .ts,.tsx",
    "lint-jsc": "jscs . --config .jscsrc",
    "lint-es": "./node_modules/.bin/eslint --config .eslintrc --ignore-path .eslintignore --ext .js,.jsx,.es6 .",
    "mongo-start": "node ./script/mongod.js",
    "mongo-stop": "bash ./script/mongod.sh stop",
    "mongo-setup": "bash ./script/mongod.sh setup",
    "start": "node ./src/global/server/server.js",
    "setup": "bash ./script/setup.sh",
    "setup-config": "./setup.js",
    "pretest": "tsc test/*.ts --module commonjs",
    "test": "mocha",
    "test-performance": "bash ./script/performance.sh",
    "test-chrometools": "bash ./script/chrometools.sh",
    "test-selenium-start": "bash ./script/selenium.sh start",
    "test-selenium-stop": "bash ./script/selenium.sh stop",
    "test-chromedriver-start": "bash ./script/chromedriver.sh start",
    "test-chromedriver-stop": "bash ./script/chromedriver.sh stop",
    "watch-client": "node ./webpack.server.client-hot.js --verbose --colors --display-error-details --projectpath $PROJECTPATH",
    "watch-server": "node ./webpack.config.server.js --verbose --colors --display-error-details",
    "watch": "npm run mongo-start; node ./node_modules/concurrently/src/main.js --kill-others \"cross-env NODE_ENV=development npm run watch-client\" \"cross-env NODE_ENV=development node src/global/server/server.js\""
  },
