{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "csione",
      "image": "974359815377.dkr.ecr.us-east-1.amazonaws.com/csi-one:test",
      "links": [],
      "update": "true",
      "environment" : [
          { "name" : "NODE_ENV", "value" : "production" }
      ],
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 80
